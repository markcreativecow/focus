<?xml version="1.0" encoding="UTF-8"?>
<!-- config.xml reference: https://build.phonegap.com/docs/config-xml -->
<widget xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0" id="com.focuseducation.conferences" version="1.0">
    <name>Conferences</name>
    <description></description>
    
    <feature name="InAppBrowser">
        <param name="ios-package" value="CDVInAppBrowser" />
    </feature>
    
    <gap:plugin name="com.phonegap.plugins.pushplugin" version="2.4.0" />    
    <gap:plugin name="com.phonegap.plugin.statusbar" version="1.1.0" />
    <gap:plugin name="org.apache.cordova.inappbrowser" />

    <gap:config-file platform="ios" parent="UIStatusBarHidden"><true/></gap:config-file>
    <gap:config-file platform="ios" parent="UIViewControllerBasedStatusBarAppearance"><false/></gap:config-file>
    <gap:config-file platform="ios" parent="CFBundleShortVersionString"><string>100</string></gap:config-file>
    
    <preference name="StatusBarOverlaysWebView" value="false" />
    
    <preference name="permissions" value="none" />
    <preference name="orientation" value="portrait" />
    <preference name="target-device" value="universal" />
    <preference name="fullscreen" value="true" />
    <preference name="webviewbounce" value="true" />
    <preference name="prerendered-icon" value="true" />
    <preference name="stay-in-webview" value="false" />
    <preference name="ios-statusbarstyle" value="black-opaque" />
    <preference name="detect-data-types" value="true" />
    <preference name="exit-on-suspend" value="false" />
    <preference name="show-splash-screen-spinner" value="true" />
    <preference name="auto-hide-splash-screen" value="true" />
    <preference name="disable-cursor" value="false" />
    <preference name="android-minSdkVersion" value="7" />
    <preference name="android-installLocation" value="auto" />
    
    <preference name="phonegap-version" value="3.7.0" />
	
    <gap:plugin name="org.apache.cordova.battery-status" />
    <gap:plugin name="org.apache.cordova.camera" />
    <gap:plugin name="org.apache.cordova.media-capture" />
    <gap:plugin name="org.apache.cordova.console" />
    <gap:plugin name="org.apache.cordova.contacts" />
    <gap:plugin name="org.apache.cordova.device" />
    <gap:plugin name="org.apache.cordova.device-motion" />
    <gap:plugin name="org.apache.cordova.device-orientation" />
    <gap:plugin name="org.apache.cordova.dialogs" />
    <gap:plugin name="org.apache.cordova.file" />
    <gap:plugin name="org.apache.cordova.file-transfer" />
    <gap:plugin name="org.apache.cordova.geolocation" />
    <gap:plugin name="org.apache.cordova.globalization" />
    <gap:plugin name="org.apache.cordova.inappbrowser" />
    <gap:plugin name="org.apache.cordova.media" />
    <gap:plugin name="org.apache.cordova.network-information" />
    <gap:plugin name="org.apache.cordova.splashscreen" />
    <gap:plugin name="org.apache.cordova.vibration" />
	
    <icon gap:platform="ios" height="40" src="res/icon/ios/appicon-Small-40.png" width="40" />
    <icon gap:platform="ios" height="80" src="res/icon/ios/appicon-Small-40@2x.png" width="80" />    
    <icon gap:platform="ios" height="50" src="res/icon/ios/appicon-Small-50.png" width="50" />
    <icon gap:platform="ios" height="100" src="res/icon/ios/appicon-Small-50@2x.png" width="100" />    
    <icon gap:platform="ios" height="29" src="res/icon/ios/appicon-Small.png" width="29" />
    <icon gap:platform="ios" height="58" src="res/icon/ios/appicon-Small@2x.png" width="58" />
    <icon gap:platform="ios" height="87" src="res/icon/ios/appicon-Small@3x.png" width="87" />    
    <icon gap:platform="ios" height="57" src="res/icon/ios/appicon.png" width="57" />
    <icon gap:platform="ios" height="114" src="res/icon/ios/appicon@2x.png" width="114" />
    <icon gap:platform="ios" height="60" src="res/icon/ios/appicon-60.png" width="60" />
    <icon gap:platform="ios" height="120" src="res/icon/ios/appicon-60@2x.png" width="120" />
    <icon gap:platform="ios" height="180" src="res/icon/ios/appicon-60@3x.png" width="180" />
    <icon gap:platform="ios" height="72" src="res/icon/ios/appicon-72.png" width="72" />
    <icon gap:platform="ios" height="144" src="res/icon/ios/appicon-72@2x.png" width="144" />
    <icon gap:platform="ios" height="76" src="res/icon/ios/appicon-76.png" width="76" />
    <icon gap:platform="ios" height="152" src="res/icon/ios/appicon-76@2x.png" width="152" />
	
    <gap:splash gap:platform="ios" height="480" src="res/screen/ios/Default.png" width="320" />
    <gap:splash gap:platform="ios" height="960" src="res/screen/ios/Default@2x.png" width="640" />
    <gap:splash gap:platform="ios" height="1136" src="res/screen/ios/Default-568h@2x.png" width="640" />
    <gap:splash gap:platform="ios" height="1024" src="res/screen/ios/Default-Portrait.png" width="768" />
    <gap:splash gap:platform="ios" height="2048" src="res/screen/ios/Default-Portrait@2x.png" width="1536" />
    <gap:splash gap:platform="ios" height="2208" src="res/screen/ios/Default-Portrait-736h@3x.png" width="1242" />
    
    <access origin="*"/>
</widget>
